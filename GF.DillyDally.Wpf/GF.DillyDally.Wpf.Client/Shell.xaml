<Window
    x:Class="GF.DillyDally.Wpf.Client.Shell"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:GF.DillyDally.Wpf.Client"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="Dilly Dally"
    Width="800"
    Height="450"
    d:DataContext="{d:DesignInstance Type={x:Type local:ShellViewModel}}"
    mc:Ignorable="d">

    <WrapPanel Orientation="Vertical">
        <TextBlock Text="Hi2" />
        <ContentControl Content="{Binding ContentBrowserViewModel, Mode=OneWay, IsAsync=True}" />
    </WrapPanel>

</Window>
