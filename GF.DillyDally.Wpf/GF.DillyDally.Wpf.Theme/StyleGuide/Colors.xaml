<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--  Backgrounds  -->
    <Color x:Key="ApplicationBackgroundColor">#EFF3F6</Color>
    <Color x:Key="ContentBackgroundColor">#FAFBFC</Color>
    <Color x:Key="FormBackgroundColor">#F0F3F8</Color>
    <Color x:Key="TopLayerBackgroundColor">#FFFFFF</Color>

    <!--  Borders  -->
    <Color x:Key="ContentSeparatorColor">#DFE2E5</Color>
    <Color x:Key="ContentInnerSeparatorColor">#E6EAEE</Color>

    <!--  Highlights  -->
    <Color x:Key="HighlightBlue">#00aaff</Color>
    <Color x:Key="HighlightOrange">#ff7800</Color>
    <Color x:Key="HighlightGreen">#1bb934</Color>
    <Color x:Key="HighlightYellow">#ffd400</Color>
    <Color x:Key="HighlightGrey">#6b6b6b</Color>


</ResourceDictionary>