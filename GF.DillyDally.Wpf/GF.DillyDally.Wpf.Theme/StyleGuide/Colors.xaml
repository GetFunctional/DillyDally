<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--  Backgrounds  -->
    <Color x:Key="Layer4BackgroundColor">#EFF3F6</Color>
    <Color x:Key="Layer3BackgroundColor">#FAFBFC</Color>
    <Color x:Key="Layer2BackgroundColor">#F0F3F8</Color>
    <Color x:Key="Layer1BackgroundColor">#FFFFFF</Color>

    <!--  Borders  -->
    <Color x:Key="Layer4BorderColor">#DFE2E5</Color>
    <Color x:Key="Layer3BorderColor">#E6EAEE</Color>
    <Color x:Key="Layer2BorderColor">#E6EAEE</Color>
    <Color x:Key="Layer1BorderColor">#E6EAEE</Color>

    <!--  Highlights  -->
    <Color x:Key="HighlightBlue">#00aaff</Color>
    <Color x:Key="HighlightOrange">#ff7800</Color>
    <Color x:Key="HighlightGreen">#1bb934</Color>
    <Color x:Key="HighlightYellow">#ffd400</Color>
    <Color x:Key="HighlightGrey">#6b6b6b</Color>


</ResourceDictionary>